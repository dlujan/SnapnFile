{
  "expo": {
    "name": "SnapnFile",
    "scheme": "snapnfileredirect",
    "slug": "SnapnFile",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.daniellujan.snapnfile",
      "buildNumber": "1.0.1"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
